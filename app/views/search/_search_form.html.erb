<div class="container">
      <div class="row">
        <div class="search-bar col-xs-7 col-xs-offset-5 ">
          <%= form_tag(search_path, method: :get) do %>
            <%= text_field_tag 'search[content]' %>
            <%= select_tag 'search[model]',options_for_select({ "user" => "customer", "book" => "item" }) %>
            <%= hidden_field_tag 'search[method]', "部分一致" => "partial" %>
            <%= submit_tag 'Search' %>
          <% end %>
        </div>
      </div> 
    </div>